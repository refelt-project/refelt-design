      "types": "./dist/lib/uikit/index.d.ts",
      "svelte": "./dist/lib/uikit/index.js",
      "default": "./dist/lib/uikit/index.js"
    },
    "./rich": {
      "types": "./dist/lib/rich/index.d.ts",
