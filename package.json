{
  "name": "refelt-design",
  "version": "1.0.30",
  "type": "module",
  "description": "Refelt Design System - Svelte UI Components",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "package": "svelte-package",
    "prepublishOnly": "npm run package"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
