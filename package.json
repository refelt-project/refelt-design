{
  "name": "refelt-design",
  "version": "1.1.4",
  "description": "Minimal, Fast, LLM-Friendly Svelte UI Kit",
  "author": "Choosewise Team",
  "license": "MIT",
  "type": "module",
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./uikit": {
      "types": "./dist/types/uikit/index.d.ts",
      "svelte": "./dist/uikit/index.js",
      "import": "./dist/uikit/index.js",
      "default": "./dist/uikit/index.js"
    },
    "./uikit/*": {
      "svelte": "./dist/uikit/*",
      "import": "./dist/uikit/*",
      "default": "./dist/uikit/*"
    },
    "./rich": {
      "types": "./dist/types/rich/index.d.ts",
      "svelte": "./dist/rich/index.js",
      "import": "./dist/rich/index.js",
      "default": "./dist/rich/index.js"
    },
    "./rich/*": {
      "svelte": "./dist/rich/*",
      "import": "./dist/rich/*",
      "default": "./dist/rich/*"
    },
    "./styles.css": "./dist/uikit/styles.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "lucide-svelte": "^0.545.0",
    "svelte": "^5.40.1",
    "svelte-spa-router": "^4.0.1"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "terser": "^5.44.0",
    "typescript": "^5.6.3",
    "vite": "^5.0.0",
    "vite-plugin-html": "^3.2.2"
  },
  "scripts": {
    "dev": "vite",
    "package": "svelte-package",
    "postpackage": "node -e \"(async()=>{const fs=await import('node:fs/promises');try{await fs.mkdir('dist/uikit',{recursive:true});await fs.copyFile('src/lib/uikit/styles.css','dist/uikit/styles.css');}catch{}})()\"",
    "build": "yarn package && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/refelt-project/refelt-design"
  },
  "homepage": "https://github.com/refelt-project/refelt-design",
  "keywords": [
    "svelte",
    "ui",
    "components",
    "design-system",
    "native-ui",
    "choosewise"
  ]
}
