      "default": "./dist/lib/rich/index.js"
    },
    "./styles.css": "./dist/lib/uikit/styles.css"
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0",
    "lucide-svelte": "^0.263.0"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^5.0.0",
